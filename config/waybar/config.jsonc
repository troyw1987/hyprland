{
  "layer": "top",
  "position": "top",
  "mod": "dock",
  "exclusive": true,
  "passthrough": false,
  "gtk-layer-shell": true,
  "height": 47,
  "modules-left": [
    "clock",
    "custom/weather",
    "hyprland/workspaces"
  ],
  "modules-center": [
    "hyprland/window"
  ],
  "modules-right": [
    "network",
    "bluetooth",
    "backlight",
    "pulseaudio",
    "temperature",
    "battery",
    "tray"
  ],
  "hyprland/workspaces": {
    "disable-scroll": true,
    "all-outputs": true,
    "on-click": "activate"
  },
  "hyprland/window": {
    "format": "{title}",
    "max-length": 50,
    "rewrite": {
      "(.*) - Mozilla Firefox": "üåé $1",
      "(.*) - vim": "ÓüÖ $1",
      "(.*) - zsh": "ÔÑ† [$1]"
    }
  },
  "custom/weather": {
    "format": "{}Û∞îÑ ",
    "tooltip": true,
    "interval": 3600,
    "exec": "wttrbar --hide-conditions --ampm --location $(curl --silent ipinfo.io/city)",
    "return-type": "json"
  },
  "tray": {
    "icon-size": 18,
    "spacing": 10
  },
  "clock": {
    "interval": 60,
    "format": "Ôê∫  {:%I:%M%p}",
    "tooltip-format": "ÔÅ≥ {:%B %e (%D)}\n<tt><small>{calendar}</small></tt>",
    "calendar": {
      "mode": "year",
      "mode-mon-col": 4,
      "format": {
        "months": "<span color='#ffead3'><b>{}</b></span>",
        "days": "<span color='#ecc6d9'><b>{}</b></span>",
        "weekdays": "<span color='#ffcc66'><b>{}</b></span>",
        "today": "<span color='#ff6699'><b>{}</b></span>"
      }
    }
  },
  "backlight": {
    "device": "intel_backlight",
    "format": "{icon}  {percent}%",
    "format-icons": [
      "Û∞Éû",
      "Û∞Éù",
      "Û∞Éü",
      "Û∞É†"
    ],
    "on-scroll-up": "brightnessctl set 1%+",
    "on-scroll-down": "brightnessctl set 1%-",
    "min-length": 6
  },
  "battery": {
    "states": {
      "good": 60,
      "warning": 30,
      "critical": 20
    },
    "format": "{icon}  {capacity}%",
    "format-discharging": "{icon} {capacity}%",
    "format-charging": "Û∞ÇÑ {capacity}%",
    "format-plugged": "Û∞ÇÑ {capacity}%",
    "format-alt": "{time} {icon}",
    "format-icons": [
      "Û±ü©",
      "Û∞Çé",
      "Û∞Å∫",
      "Û∞Åª",
      "Û∞Åº",
      "Û∞ÅΩ",
      "Û∞Åæ",
      "Û∞Åø",
      "Û∞ÇÄ",
      "Û∞ÇÅ",
      "Û∞ÇÇ",
      "Û∞Åπ",
      "Û±àë"
    ]
  },
  "pulseaudio": {
    "format": "{icon} {volume}%",
    "format-muted": "Û∞ùü",
